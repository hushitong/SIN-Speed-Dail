{
  "manifest_version": 3,
  "name": "SIN Speed Dial",
  "description": "An new tab page,fork from Yet Another Speed Dial",
  "default_locale": "zh_CN",
  "version": "1",
  "author": "hushitong@gmail.com",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "bookmarks",
    "contextMenus",
    "offscreen",
    "storage",
    "tabs",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/action-icon16.png",
      "24": "icons/action-icon24.png",
      "32": "icons/action-icon32.png"
    },
    "default_title": "Add Site to SIN Speed Dial"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "SIN-Speed-Dial@github.com",
      "strict_min_version": "59.0"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "js/*.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}